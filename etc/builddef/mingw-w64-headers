VERSION="2.0.8"
VERSEP="-";
ALIAS="mingw-w64";
CONFOPT="";
if [ "${barch:0:3}" == "win" ];then
  PREFIX="/mingw";
  STDOPTS="--prefix=${PREFIX} --enable-secure-api --enable-idl";
 elif [ "${barch:0:3}" == "cyg" ];then
  PREFIX="/usr";
  STDOPTS="--prefix=${PREFIX} --enable-w32api";
 else
  DONTBUILD="${barch}";
fi;
CFGPTH="mingw-w64-headers";
CFGBIN="mingw-w64-headers/";
RPMGROUP="dev";
