VERSION="2.24"
VERSEP="-";
PREFIX="/usr";
CONFOPT="--target=mips64-octeon-linux-gnu --disable-werror --enable-lto --enable-64-bit-bfd --enable-initfini-array --with-sysroot=${ARCHROOT}/mips64";
if [ "${barch}" != "${NARCH}" ];then
  DONTBUILD="${barch}";
fi;
