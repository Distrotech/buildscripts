VERSION="0.9.18"
VERSEP="-";
PREFIX="/usr";
if [ "${barch}" != "win64" ];then
  CONFOPT="--with-graphite2=yes";
 else
  CONFOPT="";
fi;
ADDCFLAG="-I=/usr/include/freetype2 -I=/usr/include/cairo";
case ${barch} in
  ppc64)CHROOTBUILD="0";;
  *)CHROOTBUILD="1";;
esac;
ADDCXXFLAG="-std=gnu++0x";
RPMGROUP="root";
