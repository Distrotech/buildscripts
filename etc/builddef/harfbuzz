VERSION="1.2.7"
VERSEP="-";
PREFIX="/usr";
if [ "${barch}" != "win64" ];then
  CONFOPT="--with-graphite2=yes";
 else
  CONFOPT="";
fi;
ADDCFLAG="-I=/usr/include/freetype2 -I=/usr/include/cairo";
case ${barch} in
  *)CHROOTBUILD="1";;
esac;
ADDCXXFLAG="-std=gnu++0x";
RPMGROUP="root";
SYSROOTFIXUP="0";
