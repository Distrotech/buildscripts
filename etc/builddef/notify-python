VERSION="0.1.1"
VERSEP="-";
PREFIX="/usr";
CONFOPT="";
CONFOPT="PYTHON=${ARCHROOT}/${barch}/usr/bin/python";
ADDCFLAG="-I=/usr/include/python2.7";
GITBRANCH="master";
if [ "${HASGIR}" == "no" ];then
  DONTBUILD="${barch}";
fi;
case ${barch} in
  arm|armv6|i686|x86_32|mips64|ppc*)CHROOTBUILD="1";;
esac;
RPMGROUP="core";
